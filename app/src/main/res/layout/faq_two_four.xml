<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mainBackgroud">

    <ImageView
        android:id="@+id/faqTwoFour"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:background="@drawable/faqtwofour"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/helpful"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="235dp"
        android:background="@drawable/helpful"
        app:layout_constraintStart_toStartOf="@+id/faqTwoFour"
        app:layout_constraintTop_toTopOf="@+id/faqTwoFour"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/nah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="235dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/nah"
        app:layout_constraintEnd_toEndOf="@+id/faqTwoFour"
        app:layout_constraintTop_toTopOf="@+id/faqTwoFour"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/backSymbol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginLeft="15dp"
        android:background="@drawable/backsymbol"
        app:layout_constraintEnd_toStartOf="@+id/wastageTitle"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>