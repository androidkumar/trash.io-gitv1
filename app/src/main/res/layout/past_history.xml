<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mainBackgroud">


    <ImageView
        android:id="@+id/rectPastHistory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/rectpasthistory"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/backSymbol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="44dp"
        android:background="@drawable/backsymbol"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/pastHistoryTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/rectPastHistory"
        android:layout_alignEnd="@+id/rectPastHistory"
        android:layout_alignParentTop="true"
        android:background="@drawable/pasthistorytitle"
        app:layout_constraintBottom_toBottomOf="@+id/rectPastHistory"
        app:layout_constraintEnd_toEndOf="@+id/rectPastHistory"
        app:layout_constraintStart_toStartOf="@+id/rectPastHistory"
        app:layout_constraintTop_toTopOf="@+id/rectPastHistory"
        app:layout_constraintVertical_bias="0.42000002"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/pastHistoryTab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rectPastHistory"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/pasthistorytab"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectPastHistory"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/organicDetail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rectPastHistory"
        android:layout_alignEnd="@+id/pastHistoryTab"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/organicdetail"
        app:layout_constraintEnd_toEndOf="@+id/pastHistoryTab"
        app:layout_constraintTop_toTopOf="@+id/pastHistoryTab"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/plasticDetail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rectPastHistory"
        android:layout_alignEnd="@+id/pastHistoryTab"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/organicdetail"
        app:layout_constraintEnd_toEndOf="@+id/pastHistoryTab"
        app:layout_constraintTop_toTopOf="@+id/pastHistoryTab"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/paperDetail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rectPastHistory"
        android:layout_alignEnd="@+id/pastHistoryTab"
        android:layout_marginTop="225dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/organicdetail"
        app:layout_constraintEnd_toEndOf="@+id/pastHistoryTab"
        app:layout_constraintTop_toTopOf="@+id/pastHistoryTab"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/plasticDetailTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rectPastHistory"
        android:layout_alignEnd="@+id/pastHistoryTab"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/organicdetail"
        app:layout_constraintEnd_toEndOf="@+id/pastHistoryTab"
        app:layout_constraintTop_toTopOf="@+id/pastHistoryTab"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/organicDetailTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rectPastHistory"
        android:layout_alignEnd="@+id/pastHistoryTab"
        android:layout_marginTop="425dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/organicdetail"
        app:layout_constraintEnd_toEndOf="@+id/pastHistoryTab"
        app:layout_constraintTop_toTopOf="@+id/pastHistoryTab"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/paperDetailTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rectPastHistory"
        android:layout_alignEnd="@+id/pastHistoryTab"
        android:layout_marginTop="500dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/organicdetail"
        app:layout_constraintEnd_toEndOf="@+id/pastHistoryTab"
        app:layout_constraintTop_toTopOf="@+id/pastHistoryTab"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>