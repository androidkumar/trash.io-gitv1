<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mainBackgroud">

    <TextView
        android:id="@+id/backSymbol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/backsymbol"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/reportScreen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@drawable/reportscreen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/reportDropdown"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="220dp"
        android:background="@drawable/dropdown"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/mobileAppErrors"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="125dp"
        android:background="@drawable/mobileapperrors"
        app:layout_constraintStart_toStartOf="@+id/reportDropdown"
        app:layout_constraintTop_toTopOf="@+id/reportDropdown"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/deviceLid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/devicelid"
        app:layout_constraintStart_toStartOf="@+id/reportDropdown"
        app:layout_constraintTop_toTopOf="@+id/reportDropdown"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/deviceSorting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="168dp"
        android:background="@drawable/devicesorting"
        app:layout_constraintStart_toStartOf="@+id/reportDropdown"
        app:layout_constraintTop_toTopOf="@+id/reportDropdown"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/deviceCleaning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="212dp"
        android:background="@drawable/devicecleaning"
        app:layout_constraintStart_toStartOf="@+id/reportDropdown"
        app:layout_constraintTop_toTopOf="@+id/reportDropdown"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/deviceVoice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="259dp"
        android:background="@drawable/devicevoice"
        app:layout_constraintStart_toStartOf="@+id/reportDropdown"
        app:layout_constraintTop_toTopOf="@+id/reportDropdown"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>